{
  "dashboard": {
    "id": null,
    "title": "QLens LLM Gateway Dashboard",
    "tags": ["qlens", "llm", "gateway"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "value": "${DS_PROMETHEUS}",
            "text": "Prometheus"
          }
        },
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(qlens_requests_total, tenant_id)",
          "current": {
            "value": "$__all",
            "text": "All"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "provider",
          "type": "query",
          "query": "label_values(qlens_requests_total, provider)",
          "current": {
            "value": "$__all",
            "text": "All"
          },
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(qlens_requests_total[$__rate_interval]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(qlens_requests_total{status=~\"4.*|5.*\"}[$__rate_interval])) / sum(rate(qlens_requests_total[$__rate_interval]))",
            "legendFormat": "Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.05}
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Average Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(qlens_request_duration_seconds_sum[$__rate_interval])) / sum(rate(qlens_request_duration_seconds_count[$__rate_interval]))",
            "legendFormat": "Avg Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Total Daily Cost",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(qlens_cost_usd_total[1d]))",
            "legendFormat": "Daily Cost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Request Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(qlens_requests_total[$__rate_interval])) by (service)",
            "legendFormat": "{{service}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(qlens_request_duration_seconds_bucket[$__rate_interval])) by (le))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(qlens_request_duration_seconds_bucket[$__rate_interval])) by (le))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(qlens_request_duration_seconds_bucket[$__rate_interval])) by (le))",
            "legendFormat": "p99"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Provider Request Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(qlens_provider_requests_total[$__rate_interval])) by (provider)",
            "legendFormat": "{{provider}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Provider Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(qlens_provider_latency_seconds_sum[$__rate_interval])) by (provider) / sum(rate(qlens_provider_latency_seconds_count[$__rate_interval])) by (provider)",
            "legendFormat": "{{provider}}"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Provider Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "qlens_provider_health_status",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "Unhealthy"},
              {"type": "value", "value": "1", "text": "Healthy"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 12
        }
      },
      {
        "id": 10,
        "title": "Token Usage by Model",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(qlens_tokens_processed_total[$__rate_interval])) by (model, type)",
            "legendFormat": "{{model}} - {{type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 11,
        "title": "Cost by Provider",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(qlens_cost_usd_total[$__rate_interval])) by (provider)",
            "legendFormat": "{{provider}}"
          }
        ],
        "yAxes": [
          {
            "label": "USD/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 12,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(qlens_cache_hits_total[$__rate_interval])) / (sum(rate(qlens_cache_hits_total[$__rate_interval])) + sum(rate(qlens_cache_misses_total[$__rate_interval])))",
            "legendFormat": "Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 13,
        "title": "Cache Size",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(qlens_cache_size_entries)",
            "legendFormat": "Entries"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 28
        }
      },
      {
        "id": 14,
        "title": "Rate Limit Hits",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(qlens_rate_limit_hits_total[$__rate_interval])) by (limit_type)",
            "legendFormat": "{{limit_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Hits/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": 15,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "qlens_cpu_usage_percentage",
            "legendFormat": "CPU %"
          },
          {
            "expr": "qlens_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory MB"
          }
        ],
        "yAxes": [
          {
            "label": "Usage",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 36
        }
      }
    ],
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1
  }
}